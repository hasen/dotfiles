[color]        
  diff        = auto
  status      = auto
  branch      = auto
  interactive = auto
  grep        = auto
[alias]
  st  = status -sb
  ad  = add
  ci  = commit
  cm  = commit -m
  cam = commit -am
  br  = branch -a
  lp  = log -p
  gr  = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
  di  = diff --color-words
  gp  = grep -n 
  bl  = blame
